<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Lab5.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Lab5.Views.DialogAddWindow"
        x:DataType="viewModels:DialogAddViewModel"
        Title="DialogWindow" Width="220" Height="300">
        
        <StackPanel>
                <Label>Track</Label>
                <TextBox Text="{Binding Title}" Width="200" Height="50" Margin="0 10 0 0 " Name="Title"></TextBox>
                <Label>Author</Label>
                <TextBox Text="{Binding Author}" Width="200" Height="50" Margin="0 10 0 0 " Name="Author"></TextBox>
                <Button Width="200" Height="50" Margin="10" Command="{Binding AddTrack}"
                        VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Name="OkButton">OK</Button>
        </StackPanel>
</Window>
